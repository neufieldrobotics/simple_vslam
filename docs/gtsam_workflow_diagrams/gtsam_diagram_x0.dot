  x1 -> l3 [style=invis color=blue]
  x1 -> l4 [style=invis color=blue]

  x2 -> l2 [style=invis]
  x2 -> l3 [style=invis]
  x2 -> l4 [style=invis]

  x2 -> l5 [style=invis]

  x3 -> l4 [style=invis]
  x3 -> l5 [style=invis]

  x3 -> l6 [style=invis]
  x3 -> l7 [style=invis]

}

